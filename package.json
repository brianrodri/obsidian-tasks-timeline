{
    "name": "obsidian-tasks-timeline",
    "version": "1.0.0",
    "description": "This is a sample plugin for Obsidian (https://obsidian.md)",
    "main": "dist/main.js",
    "scripts": {
        "test": "vitest run",
        "watch": "vitest --watch",
        "coverage": "vitest run --coverage",
        "watch-coverage": "vitest --watch --coverage",
        "hotreload": "vite build --mode development --outDir ./test-vault/.obsidian/plugins/obsidian-tasks-timeline/ --watch",
        "build": "tsc && vite build",
        "docs": "typedoc src/main.tsx --plugin typedoc-plugin-coverage --out docs/ --excludeExternals --excludePrivate --excludeProtected",
        "lint": "eslint .",
        "prepare": "node .husky/install.js",
        "commitlint": "commitlint --edit",
        "clean": "rm -rf node_modules/ dist/ coverage/ docs/"
    },
    "keywords": [],
    "author": "",
    "license": "MIT",
    "type": "module",
    "dependencies": {
        "@preact/signals": "^2.0.1",
        "core-js": "^3.40.0",
        "lodash": "^4.17.21",
        "lucide-preact": "^0.474.0",
        "luxon": "^3.5.0",
        "obsidian": "^1.7.2",
        "preact": "^10.25.4",
        "react": "npm:@preact/compat",
        "react-dom": "npm:@preact/compat",
        "usehooks-ts": "^3.1.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^19.6.1",
        "@commitlint/config-conventional": "^19.6.0",
        "@eslint/eslintrc": "^3.2.0",
        "@eslint/js": "^9.19.0",
        "@preact/preset-vite": "^2.10.0",
        "@semantic-release/git": "^10.0.1",
        "@testing-library/preact": "^3.2.4",
        "@testing-library/user-event": "^14.6.1",
        "@trivago/prettier-plugin-sort-imports": "^5.2.1",
        "@types/core-js": "^2.5.8",
        "@types/lodash": "^4.17.14",
        "@types/luxon": "^3.4.2",
        "@typescript-eslint/eslint-plugin": "^8.22.0",
        "@typescript-eslint/parser": "^8.22.0",
        "@vitest/coverage-v8": "^3.0.4",
        "eslint": "^9.19.0",
        "eslint-config-preact": "github:brianrodri/eslint-config-preact#eslint-v9",
        "eslint-config-prettier": "^10.0.1",
        "eslint-plugin-boundaries": "^5.0.1",
        "eslint-plugin-prettier": "^5.2.3",
        "eslint-plugin-react": "^7.37.4",
        "eslint-plugin-react-hooks": "^5.1.0",
        "globals": "^15.14.0",
        "husky": "^9.1.7",
        "jsdom": "^26.0.0",
        "obsidian": "^1.7.2",
        "obsidian-dataview": "^0.5.67",
        "semantic-release": "^24.2.1",
        "semantic-release-obsidian-plugin": "^1.1.0",
        "tslib": "^2.8.1",
        "typedoc": "^0.27.6",
        "typedoc-plugin-coverage": "^3.4.1",
        "typescript": "^5.7.3",
        "utility-types": "^3.11.0",
        "vite": "^6.0.11",
        "vite-plugin-static-copy": "^2.2.0",
        "vitest": "^3.0.4"
    },
    "overrides": {
        "obsidian-calendar-ui": "brianrodri/obsidian-calendar-ui"
    }
}
