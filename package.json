{
    "name": "obsidian-tasks-timeline",
    "version": "1.0.0",
    "description": "This is a sample plugin for Obsidian (https://obsidian.md)",
    "main": "dist/main.js",
    "scripts": {
        "test": "vitest run",
        "watch": "vitest --watch",
        "coverage": "vitest run --coverage",
        "watch-coverage": "vitest --watch --coverage",
        "hotreload": "vite build --mode development --outDir ./test-vault/.obsidian/plugins/obsidian-tasks-timeline/ --watch",
        "build": "tsc && vite build",
        "docs": "typedoc src/main.tsx --plugin typedoc-plugin-coverage --out docs/ --excludeExternals --excludePrivate --excludeProtected",
        "lint": "eslint .",
        "prepare": "node .husky/install.js",
        "commitlint": "commitlint --edit",
        "clean": "rm -rf node_modules/ dist/ coverage/ docs/"
    },
    "keywords": [],
    "author": "",
    "license": "MIT",
    "type": "module",
    "dependencies": {
        "@preact/signals": "^2.3.1",
        "core-js": "^3.45.1",
        "lodash": "^4.17.21",
        "lucide-preact": "^0.544.0",
        "luxon": "^3.7.2",
        "obsidian": "^1.8.7",
        "preact": "^10.27.2",
        "react": "npm:@preact/compat",
        "react-dom": "npm:@preact/compat",
        "usehooks-ts": "^3.1.1"
    },
    "devDependencies": {
        "@commitlint/cli": "^20.1.0",
        "@commitlint/config-conventional": "^20.0.0",
        "@eslint/eslintrc": "^3.3.1",
        "@eslint/js": "^9.36.0",
        "@preact/preset-vite": "^2.10.2",
        "@semantic-release/git": "^10.0.1",
        "@testing-library/preact": "^3.2.4",
        "@testing-library/user-event": "^14.6.1",
        "@trivago/prettier-plugin-sort-imports": "^5.2.2",
        "@types/core-js": "^2.5.8",
        "@types/lodash": "^4.17.20",
        "@types/luxon": "^3.7.1",
        "@typescript-eslint/eslint-plugin": "^8.45.0",
        "@typescript-eslint/parser": "^8.45.0",
        "@vitest/coverage-v8": "^3.2.4",
        "eslint": "^9.36.0",
        "eslint-config-preact": "github:brianrodri/eslint-config-preact#eslint-v9",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-boundaries": "^5.0.1",
        "eslint-plugin-prettier": "^5.5.4",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^5.2.0",
        "globals": "^16.4.0",
        "husky": "^9.1.7",
        "jsdom": "^27.0.0",
        "obsidian": "^1.8.7",
        "obsidian-dataview": "^0.5.68",
        "semantic-release": "^24.2.9",
        "semantic-release-obsidian-plugin": "^1.2.0",
        "tslib": "^2.8.1",
        "typedoc": "^0.28.13",
        "typedoc-plugin-coverage": "^4.0.1",
        "typescript": "^5.9.3",
        "utility-types": "^3.11.0",
        "vite": "^7.1.7",
        "vite-plugin-static-copy": "^3.1.2",
        "vitest": "^3.2.4"
    },
    "overrides": {
        "obsidian-calendar-ui": "brianrodri/obsidian-calendar-ui"
    }
}
